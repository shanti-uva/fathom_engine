<div id="main">
<div id="secondary-nav" class="yui-navset">
	<div id="main-content"  class="narrow">
<h1>New blacklist</h1>

<% form_for(@blacklist) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.label :attempts %><br />
    <%= f.text_field :attempts %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', blacklists_path %>


  </div> <!--END CONTENT-->
</div> <!-- END SECONDARY NAV -->
</div><!--END MAIN-->