<div id="main">
<%= error_messages_for :organization %>
<h3>Create an Organization</h3>
<% form_for(@organization, :html => { :multipart => true }) do |f| %>

	<table class="entity-list-table">
		<tr>
			<td><b>Organization Name:</b></td>
			<td><%= f.text_field "name" %></td>
		</tr>
		<tr>
			<td><b><label for="image">Upload Image:</label></b></td>
			<td><%= f.file_field :uploaded_data %></td>
		</tr>
	</table>
	
    <%= f.submit "Create" %>
<% end %>
</div>
